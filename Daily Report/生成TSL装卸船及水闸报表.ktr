    <sql>SELECT PC.CNTR AS CNTR_NO,&#xd;&#xa;       PC.SEAL_NO AS SEAL_NO1,&#xd;&#xa;       &#x27;&#x27; AS SEAL_NO2,&#xd;&#xa;       &#x27;&#x27; AS SEAL_NO3,&#xd;&#xa;       PC.E_F_ID AS FE,&#xd;&#xa;       PC.CNTR_CLASS AS IX_CD,&#xd;&#xa;       PC.CNTR_CORP_COD PTNR_CODE,&#xd;&#xa;       PC.CNTR_ISO AS SZTP2,&#xd;&#xa;       PC.CNTR_SIZ_COD &#x7c;&#x7c; PC.CNTR_TYP_COD AS GENTYPE,&#xd;&#xa;       PC.DAM_DISCRIBE AS DMG_COND,&#xd;&#xa;       &#x27;&#x27; AS REMARK,&#xd;&#xa;       PC.BILL_NO AS BL_NO,&#xd;&#xa;       PC.BOOKING_NO AS BOOKING_NO,&#xd;&#xa;       PC.CNTR_GROSS_WGT AS WGT,&#xd;&#xa;       CASE&#xd;&#xa;          WHEN PC.E_F_ID &#x3d; &#x27;E&#x27; THEN &#x27;MT&#x27;&#xd;&#xa;          WHEN PC.E_F_ID &#x3d; &#x27;F&#x27; THEN &#x27;GP&#x27;&#xd;&#xa;          WHEN PC.E_F_ID &#x3d; &#x27;F&#x27; AND PC.REEF_ID &#x3d; &#x27;1&#x27; THEN &#x27;RF&#x27;&#xd;&#xa;          WHEN PC.E_F_ID &#x3d; &#x27;F&#x27; AND PC.DANG_ID &#x3d; &#x27;1&#x27; THEN &#x27;DG&#x27;&#xd;&#xa;          WHEN PC.E_F_ID &#x3d; &#x27;F&#x27; AND PC.OOG_ID &#x3d; &#x27;1&#x27; THEN &#x27;AK&#x27;&#xd;&#xa;          ELSE &#x27;&#x27;&#xd;&#xa;       END&#xd;&#xa;          AS CARGO_TYPE,&#xd;&#xa;       F_GET_SHIPLINE &#x28;PC.IN_TOOL_NO, PC.TRADE_ID&#x29; AS IN_LINE,&#xd;&#xa;       F_GET_SHIPLINE &#x28;PC.OUT_TOOL_NO, PC.TRADE_ID&#x29; AS OUT_LINE,&#xd;&#xa;       &#x27;&#x27; AS YARDPOSITION,&#xd;&#xa;       &#x27;&#x27; AS AREA,&#xd;&#xa;       &#x27;&#x27; AS DISPATCH_MODE,&#xd;&#xa;       D.E_SHIP_NAM AS VSL_NM,&#xd;&#xa;       S.VOYAGE AS IN_VOY,&#xd;&#xa;       PC.LOAD_PORT_COD AS POL,&#xd;&#xa;       PC.TRANS_PORT_COD AS POD,&#xd;&#xa;       PC.IN_PORT_TIM AS IN_TIMNL_DATE,&#xd;&#xa;       ROUND &#x28;&#xd;&#xa;          &#x28;CASE&#xd;&#xa;              WHEN PC.OUT_PORT_TIM IS NULL&#xd;&#xa;              THEN&#xd;&#xa;                 SYSDATE - NVL &#x28;PC.IN_PORT_TIM, PC.IN_CY_TIM&#x29; &#x2b; 1&#xd;&#xa;              ELSE&#xd;&#xa;                 PC.OUT_PORT_TIM - PC.IN_PORT_TIM &#x2b; 1&#xd;&#xa;           END&#x29;,&#xd;&#xa;          1&#x29;&#xd;&#xa;          AS STACKDAYS,&#xd;&#xa;       &#x27;&#x27; AS DOMESTIC_CHK,&#xd;&#xa;       &#x27;&#x27; AS CNTR_STATE,&#xd;&#xa;       PC.TEMP_SET AS SET_TEMP,&#xd;&#xa;       PC.DANG_CLASS AS IMDG,&#xd;&#xa;       PC.UNDG_NO AS UNNO,&#xd;&#xa;       &#x27;&#x27; AS CNTR_COND,&#xd;&#xa;       PC.CNTR_FLOW AS STORAGE_CODE,&#xd;&#xa;	   &#x27;NICT&#x27; AS TERMINAL_CODE&#xd;&#xa;  FROM PORT_CNTR PC&#xd;&#xa;       LEFT JOIN SHIP S ON &#x28;PC.SHIP_NO &#x3d; S.SHIP_NO&#x29;&#xd;&#xa;       LEFT JOIN C_SHIP_DATA D ON &#x28;S.SHIP_COD &#x3d; D.SHIP_COD&#x29;&#xd;&#xa; WHERE     PC.CNTR_CORP_COD &#x3d; &#x27;TSL&#x27;&#xd;&#xa;       AND DAM_ID &#x3d; &#x27;1&#x27;&#xd;&#xa;</sql>
