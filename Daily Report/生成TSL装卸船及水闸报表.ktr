    <sql>SELECT TI.CNTR_NO,&#xd;&#xa;       TI.SEAL_NO1,&#xd;&#xa;       TI.SEAL_NO2,&#xd;&#xa;       TI.SEAL_NO3,&#xd;&#xa;       TI.FE,&#xd;&#xa;       TI.IX_CD,&#xd;&#xa;       TI.PTNR_CODE,&#xd;&#xa;       TI.SZTP2 AS SZTP,&#xd;&#xa;       TG.PRI_CODE AS GenType,&#xd;&#xa;       TI.DMG_COND,&#xd;&#xa;       TI.REMARK,&#xd;&#xa;       TI.BL_NO,&#xd;&#xa;       TI.BOOKING_NO,&#xd;&#xa;       TI.WGT,&#xd;&#xa;       TI.CARGO_TYPE,&#xd;&#xa;       TB.IN_LANE,&#xd;&#xa;       TB.OUT_LANE,&#xd;&#xa;       &#x27;&#x27; AS YardPosition,&#xd;&#xa;       TI.AREA,&#xd;&#xa;       TI.DISPATCH_MODE,&#xd;&#xa;       TV.VSL_NM,&#xd;&#xa;       TB.IN_VOY,&#xd;&#xa;       TI.POL,&#xd;&#xa;       TI.POD,&#xd;&#xa;       TI.IN_TMNL_DATE,&#xd;&#xa;       round&#x28;sysdate - TI.in_tmnl_date, 1&#x29; AS StackDays,&#xd;&#xa;       TI.DOMESTIC_CHK,&#xd;&#xa;       TI.CNTR_STATE,&#xd;&#xa;       TI.SET_TEMP,&#xd;&#xa;       TI.IMDG,&#xd;&#xa;       TI.UNNO,&#xd;&#xa;       TI.CNTR_COND,&#xd;&#xa;       TI.STORAGE_CODE,&#xd;&#xa;       &#x27;GOCT&#x27; AS TERMINAL_CODE&#xd;&#xa;  from pcatos.tb_inventory TI&#xd;&#xa;  LEFT OUTER JOIN &#x28;SELECT TB_GSZTP.&#x2a;&#xd;&#xa;                     FROM PCATOS.TB_GSZTP&#xd;&#xa;                    WHERE &#x28;ISO_TYPE &#x3d; &#x27;&#x2a;&#x27;&#x29;&#x29; TG&#xd;&#xa;    ON TI.SZTP2 &#x3d; TG.ISO_CODE&#xd;&#xa;  LEFT OUTER JOIN PCATOS.TB_VSL TV&#xd;&#xa;    ON TI.VSL_CD &#x3d; TV.VSL_CD&#xd;&#xa;  LEFT OUTER JOIN PCATOS.TB_BERTHPLAN TB&#xd;&#xa;    ON &#x28;TI.VSL_CD &#x3d; TB.VSL_CD AND TI.CALL_YEAR &#x3d; TB.CALL_YEAR AND&#xd;&#xa;       TI.CALL_SEQ &#x3d; TB.CALL_SEQ&#x29;&#xd;&#xa; WHERE &#x28;TI.DMG_COND &#x3d; &#x27;R&#x27; OR TI.DMG_COND &#x3d; &#x27;M&#x27; OR TI.DMG_COND &#x3d; &#x27;H&#x27; OR&#xd;&#xa;       TI.DMG_COND &#x3d; &#x27;T&#x27;&#x29;&#xd;&#xa;   AND &#x28;TI.PTNR_CODE &#x3d; &#x27;TSL&#x27;&#x29;&#xd;&#xa;</sql>
    <limit>0</limit>
    <lookup/>
